{
	"variables": {
		"aws_access_key": "",
		"aws_secret_key": ""
	},
	"builders": [{
		"type": "amazon-ebs",
		"access_key": "{{user `aws_access_key`}}",
		"secret_kay": "{{user `aws_secret_key`}}",
		"region": "us-east-1",
		"source_ami": "ami-07ebfd5b3428b6f4d",
		"instance_type": "t2.micro",
		"ssh_username": "ubuntu",
		"ami_name": "packer-example {{timestamp}}"
	}],
	"provisioners": [{
		"type": "shell",
		"scripts": ["scripts/install_software.sh"],
		"execute_command": "{{ .Vars }} sudo -E sh '{{ .Path }}'",
		"pause_before": "10s"
	}]
}